{
  "version": 2,
  "alias": [
    "toggl-ical.kattcorp.com",
    "toggl-ical.kattcorp.se",
    "toggl-ical.kattcorp.co.uk"
  ],
  "builds": [
    {
      "src": "./src/**/*.ts",
      "use": "@now/node"
    }
  ],
  "env": {
    "IS_NOW": "true",
    "TOGGL_API_TOKEN": "@toggl-api-token"
  },
  "routes": [{ "src": "/", "dest": "/src" }]
}
