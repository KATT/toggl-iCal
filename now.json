{
  "version": 2,
  "alias": [
    "toggl-ical.kattcorp.com",
    "toggl-ical.kattcorp.se",
    "toggl-ical.kattcorp.co.uk"
  ],
  "builds": [
    {
      "src": "./src/**/*.ts",
      "use": "@now/node"
    }
  ],
  "env": {
    "IS_NOW": "true"
  },
  "routes": [
    {
      "src": "/",
      "dest": "/src"
    },
    {
      "src": "/index.json",
      "dest": "/src"
    }
  ]
}